{
  "name": "stackoverflow-lite",
  "version": "1.0.0",
  "description": "A mini version of stackoverflow-lite",
  "main": "index.js",
  "scripts": {
    "test": "nyc mocha test/*.js",
    "lint": "eslint **/*.js --fix",
    "coverage":"nyc --reporter=lcov --reporter=text -icov npm test",
    "start": "npm run build && nodemon src/index.js --exec babel-node",
    "build": "babel pretest -d test & babel src -d dist",
    "serve": "node dist/index.js"
